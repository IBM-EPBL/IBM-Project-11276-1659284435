int x,y,t ;
void setup ()
{
  pinMode( 8, INPUT);
  pinMode( 5, OUTPUT);
  pinMode (A5, INPUT);
  pinMode (6,OUTPUT);
  pinMode(4,OUTPUT);
  pinMode(A4,INPUT);
  Serial.begin(9600);
}
void loop ()
{
  float temp = analogRead(A4);
  float t = map(analogRead(A4),20,358,-40,125);
  if(t>27)
  {
    digitalWrite(4,HIGH);
  }
